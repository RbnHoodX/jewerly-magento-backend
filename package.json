{
  "name": "shopify-database-sync",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "sync:once": "tsx src/index.ts --once",
    "cleanup:duplicates": "tsx src/cleanup-duplicates.ts",
    "cleanup:tracking-notes": "tsx src/cleanup-tracking-notes.ts",
    "cleanup": "tsx src/cleanup-supabase.ts",
    "automation:start": "tsx src/scripts/automation-cli.ts start",
    "automation:stop": "tsx src/scripts/automation-cli.ts stop",
    "automation:run-once": "tsx src/scripts/automation-cli.ts run-once",
    "automation:status": "tsx src/scripts/automation-cli.ts status",
    "automation:test": "tsx src/scripts/test-automation.ts",
    "test:migrations": "tsx src/scripts/test-migrations.ts",
    "test:structure": "tsx src/scripts/test-structure.ts",
    "test:email": "tsx src/scripts/test-real-email.ts",
    "test:gmail": "tsx src/scripts/test-gmail-setup.ts",
    "lint": "eslint ."
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.46.1",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "tslib": "^2.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "eslint": "^8.57.0",
    "tsx": "^4.16.2",
    "typescript": "^5.6.2"
  }
}
