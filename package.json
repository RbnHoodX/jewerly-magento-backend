{
  "name": "shopify-database-sync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && cp -r src/config dist/ 2>/dev/null || true",
    "build:dev": "tsc",
    "build:bun": "bun build src/api/server.ts --outdir dist --target bun --minify",
    "start": "node dist/api/server.js",
    "start:bun": "bun run dist/server.js",
    "dev": "bun --watch src/api/server.ts",
    "sync:once": "tsx src/index.ts --once",
    "cleanup:duplicates": "tsx src/cleanup-duplicates.ts",
    "cleanup:tracking-notes": "tsx src/cleanup-tracking-notes.ts",
    "cleanup": "tsx src/cleanup-supabase.ts",
    "automation:start": "tsx src/scripts/automation-cli.ts start",
    "automation:stop": "tsx src/scripts/automation-cli.ts stop",
    "automation:run-once": "tsx src/scripts/automation-cli.ts run-once",
    "automation:status": "tsx src/scripts/automation-cli.ts status",
    "automation:test": "tsx src/scripts/test-automation.ts",
    "test:migrations": "tsx src/scripts/test-migrations.ts",
    "test:structure": "tsx src/scripts/test-structure.ts",
    "test:email": "tsx src/scripts/test-real-email.ts",
    "test:gmail": "tsx src/scripts/test-gmail-setup.ts",
    "test:email-fix": "tsx src/scripts/test-email-fix.ts",
    "test:simple-email": "tsx src/scripts/simple-email-test.ts",
    "test:wait-times": "tsx src/scripts/test-wait-times.ts",
    "backfill:customer-ids": "tsx src/scripts/backfill-customer-ids.ts",
    "test:customer-ids": "tsx src/scripts/test-customer-id-generation.ts",
    "update:existing-orders": "tsx src/scripts/update-existing-orders.ts",
    "backfill:order-datetimes": "tsx src/scripts/backfill-order-datetimes.ts",
    "check:discount-columns": "tsx src/scripts/check-discount-columns.ts",
    "check:shipping-cost-column": "tsx src/scripts/add-shipping-cost-column.ts",
    "backfill:discounts-shipping": "tsx src/scripts/backfill-discounts-and-shipping.ts",
    "check:order-ids": "tsx src/scripts/check-order-ids.ts",
    "api:start": "tsx src/api/server.ts",
    "api:dev": "tsx --watch src/api/server.ts",
    "lint": "eslint .",
    "import-diamonds": "tsx scripts/import-diamonds-inventory.ts"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.46.1",
    "@types/papaparse": "^5.3.16",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "googleapis": "^159.0.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "papaparse": "^5.5.3",
    "tslib": "^2.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "@types/nodemailer": "^7.0.1",
    "eslint": "^8.57.0",
    "tsx": "^4.16.2",
    "typescript": "^5.6.2"
  }
}
